!function($){$('.pl-sn-navpro').on('template_ready',function(){$.plNavPro.init($(this))
$.plNavPro.push($(this))})
$.plNavPro={init:function(section){var that=this
section.find('.navpro-mob-menu').each(function(e){$(this).burgermenu({position:'right',toggle_selector:'#nav-mob-toggle',parent_selector:'.menu-item-has-children',children_selector:'.sub-menu'})
$(this).find('.sub-menu').append('<li class="back"><a href="#"><i class="pl-icon pl-icon-angle-left"></i> Back</a></li>');$(this).find('.menu-item-has-children a').append(' <i class="pl-icon pl-icon-angle-right"></i>')})
var menuButton=$('#nav-mob-toggle');menuButton.on("click touchstart",function(e){$(this).toggleClass('is-active');e.preventDefault()});$('.navpro-mob-menu .menu-item:not(".menu-item-has-children") a').on('click',function(){menuButton.trigger('click')})
var navproSearch=$('#pl-navpro-search')
navproSearchOpen=$('#search-open')
navproSearchClose=$('#search-close')
navproSearchOpen.add(navproSearchClose).on("click",function(){navproSearch.toggleClass('show-search');$('.search-field').focus()});$(document).keyup(function(e){if(e.keyCode==27){if(navproSearch.hasClass('show-search')){navproSearch.toggleClass('show-search')}}})},push:function(section){var sticky=section.find('.navpro-wrap.do-fixed').length||!1
var adminbar=$('#wpadminbar')
if(!1!==sticky){$('body').css('top',section.find('nav').height())
$(section.find('nav')).css('top',adminbar.height())
$(section.find('.pldd-bar')).css('top','0').css('position','fixed')}}}}(window.jQuery)